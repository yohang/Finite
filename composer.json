{
    "name": "yohang/finite",
    "description": "A simple PHP Finite State Machine",
    "keywords": [
        "statemachine",
        "workflow",
        "state",
        "transition",
        "symfony",
        "bundle"
    ],
    "homepage": "https://github.com/yohang/Finite",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yohan Giarelli",
            "email": "yohan@giarel.li",
            "homepage": "http://yohan.giarel.li"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/property-access": "^6.1|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.26",
        "symfony/var-dumper": "^6.1|^7.0",
        "twig/twig": "^3.4",
        "vimeo/psalm": "^4.30"
    },
    "autoload": {
        "psr-4": {
            "Finite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Finite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-future": "2.0.x-dev"
        }
    }
}
